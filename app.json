{
  "expo": {
    "name": "my-app-localization",
    "slug": "my-app-localization",
    "version": "1.0.0",
    "plugins": [
      "expo-localization"
    ],
    "ios": {
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
  },
  "extra": {
      "supportsRTL": true,
      "forcesRTL": true
    }
}
}
